# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'BPForEveryone' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BPForEveryone
  pod 'CorePlot', '~> 2.2'
  pod 'Charts', '~> 3.0'

end

target 'BPForProfessionals' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BPForProfessionals
  pod 'CorePlot', '~> 2.2'
  pod 'Charts', '~> 3.0'

end
